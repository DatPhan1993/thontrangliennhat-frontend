{
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_BASE_URL": "https://thontrangliennhat-api.vercel.app",
    "REACT_APP_API_URL": "https://thontrangliennhat-api.vercel.app/api"
  }
} 